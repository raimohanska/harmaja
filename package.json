{
  "name": "harmaja",
  "version": "0.9.0",
  "main": "lonna/index.js",
  "types": "lonna/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "babel-jest": "^26.3.0",
    "csstype": "^3.0.2",
    "jest": "^26.4.0",
    "lonna": "^0.1.11",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.2.0",
    "typescript": "^4"
  },
  "dependencies": {
    "baconjs": "^3.0.15"
  },
  "peerDependencies": {
    "lonna": "^0.1.11"
  },
  "engines": {
    "node": ">13.0"
  },
  "scripts": {
    "example-first": "yarn build && parcel --out-dir dist/examples examples/first/index.html",
    "example-search": "yarn build && parcel --out-dir dist/examples examples/search/index.html",
    "example-todoapp": "yarn build && parcel --out-dir dist/examples examples/todoapp/index.html",
    "example-todoapp-atoms": "yarn build && parcel --out-dir dist/examples examples/todoapp-atoms/index.html",
    "example-todoapp-backend": "yarn build && parcel --out-dir dist/examples examples/todoapp-backend/index.html",
    "example-consultants": "yarn build && mkdir -p dist/examples && cp -R examples/consultants/public/* dist/examples && parcel --out-dir dist/examples examples/consultants/index.html",
    "example-side-effects": "yarn build && parcel --out-dir dist/examples examples/side-effects/index.html",
    "tsc": "tsc",
    "build": "scripts/build lonna && scripts/build bacon",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "switch-observables": "scripts/switch-observables $1"
  }
}
